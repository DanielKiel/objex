{
    "require": {
        "symfony/http-foundation": "^4.0",
        "symfony/routing": "^4.0",
        "symfony/http-kernel": "^4.0",
        "phpunit/phpunit": "^6.5",
        "symfony/event-dispatcher": "^4.0",
        "symfony/dependency-injection": "^4.0",
        "symfony/console": "^4.0",
        "doctrine/orm": "^2.6",
        "symfony/var-dumper": "^4.0",
        "symfony/expression-language": "^4.0",
        "monolog/monolog": "^1.23",
        "symfony/filesystem": "^4.0",
        "symfony/finder": "^4.0",
        "symfony/stopwatch": "^4.0",
        "symfony/security": "^4.0",
        "guzzlehttp/guzzle": "^6.3",
        "symfony/dotenv": "^4.0",
        "defuse/php-encryption": "^2.1",
        "symfony/validator": "^4.0",
        "symfony/cache": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "": "src/",
            "Objex\\Validation\\": "src/modules/Validation",
            "Objex\\ExpressionLanguage\\": "src/modules/ExpressionLanguage",
            "Objex\\Logger\\": "src/modules/Logger",
            "Objex\\DBStorage\\": "src/modules/DBStorage",
            "Objex\\Security\\": "src/modules/Security"
        },
        "files": [
            "src/util/pathhelper.php",
            "src/config/database.php",
            "src/config/modules.php",
            "src/config/schema.php",
            "src/bootstrap/services.php"
